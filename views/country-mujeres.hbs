          <div class="contract-button">
            <a href="/contratos?financiador={{result.compiledRelease.name}}" class="button-link">Ver todas las personas</a>
          </div>
        </div>
        <div class="col-12 col-m-12 mt-2">
          <hr>
        </div>
      </div>
    </div>
  </div>
</section>

