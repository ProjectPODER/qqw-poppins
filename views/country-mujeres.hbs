            <script type="text/javascript" src="/javascripts/country-mujeres-graph.js"></script>

          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<section class="country-mujeres" id="consejosdehombres">
  <div class="row">
    <div class="content">
      <div class="col-9 col-m-9 bg-green">
        <div class="variations-container row">
            <div class="col-m-12 col-12">
                <h4 class="description-data-subtitle text-white"><i class="fas fa-mars"></i> CONSEJOS DE HOMBRES</h4>
                <div class="row overflow-cards scroller">
                  {{#each result.summaries.2020.consejos_de_hombres }}
                    <div class="man-cards">
                        <h4><b><a href="/personas/{{id}}"> {{name}}</a></b></h4>
