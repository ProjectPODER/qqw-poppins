<div class="container">
  <div class='viewWrapper'>
    <div class='datos-generales clearfix'>
      <div class="gd-col-left-wrapper col-m-12">
        {{!-- {{#if result.data.records.0.compileRelease.contracts.0.title}} --}}
          <div class="description-data-title">
            <h3>
              <img src="../images/icons/contrato-gray.svg" alt="contrato" class="icon-lf">
              {{result.data.records.0.compiledRelease.contracts.0.title}}
            </h3>
            <div class="col-m-6 col-6">
              <h4 class="contract-type">Contrato</h4>
            </div>
            <div class="col-m-6 col-6 right-align">
              <p class="amount-data">
                {{format_amount result.amount}}
              </p>
              {{format_currency result.currency}}
            </div>
          </div>
        {{!-- {{/if}} --}}
      </div>
    </div>
    <div class="col-12 col-m-12">
      <hr>
    </div>
    <div class="contract_detail clearfix">

      <div class="col-m-9  col-9flex mt-2" id="datosgenerales">
        <div class="col-m-12">
          <h4 class="description-data-subtitle">DATOS GENERALES</h4>
        </div>
        {{#if result.start_date }}
        <div class="col-m-4 col-4">
            <p class="js-title-data">Fecha de inicio</p>
            <p class="js-data">{{moment result.start_date date lang="es" format="LL"}}</p>
        </div>
        {{/if}}
        {{#if result.end_date }}
        <div class="col-m-8 col-8">
            <p class="js-title-data">Fecha de fin</p>
            <p class="js-data">{{moment result.end_date date lang="es" format="LL"}}</p>
        </div>
        {{/if}}
        {{#if result.dependency }}
        <div class="col-m-12 col-12 link-lf-contracts">
            <p class="js-title-data">Dependencia <a data-toggle="tooltip" data-placement="right" title="Nombre de la Dependencia, Entidad Federativa o Municipio que realiza la contratación."><i class="fa fa-question"></i></a></p>
            <a href="/orgs/{{result.dependency}} {{!-- {{simpleName (getField object=parties field="parent")}} --}}"><p class="js-data">{{result.dependency}} {{!-- {{getField object=parties field="parent"}} --}}</p></a>
        </div>
        {{/if}}
        {{#if buyer.name }}
        <div class="col-m-7 col-7">
            <p class="js-title-data">Unidad compradora <a data-toggle="tooltip" data-placement="right" title="El área  de  las dependencias o entidades que ha sido  registrada y autorizada para realizar operaciones y llevar a cabo procedimientos de contratación en CompraNet."><i class="fa fa-question"></i></a></p>
            <p class="js-data">{{buyer.name}}</p>
        </div>
        {{/if}}
        {{#if buyer.id }}
        <div class="col-m-5 col-5">
            <p class="js-title-data">Clave de unidad compradora <a data-toggle="tooltip" data-placement="right" title="Clave de identificación de la Unidad Compradora en Compranet 3.0."><i class="fa fa-question"></i></a></p>
            <p class="js-data">{{buyer.id}}</p>
        </div>
        {{/if}}
        {{#if contactPoint }}
        <div class="col-m-12 col-12 link-lf-contracts">
            <p class="js-title-data">Responsable <a data-toggle="tooltip" data-placement="right" title="Titular del área contratante o del área responsable de la contratación a nivel central o del servidor público que éste designe."><i class="fa fa-question"></i></a></p>
            <p class="js-data">{{!-- {{getField object=parties field="contactPoint"}} --}}</p>
        </div>
        {{/if}}
        {{#if result.suppliers_org }}
        <div class="col-m-12 col-12 link-lf-contracts">
            <p class="js-title-data">Proveedor</p>
            <a href="/orgs/{{!-- {{awards.[0].suppliers.[0].id}} --}}"><p class="js-data">{{!-- {{awards.[0].suppliers.[0].name}} --}}{{result.suppliers_org}}</p></a>
        </div>
        {{/if}}
        <hr>
      </div>
      <div class="col-m-3 col-3">
        <div id="right-menu-contracts">
              <ul class="right-menu-contracts nav-stacked">
                  <li>
                      <a class="page-scroll" href="#datosgenerales">Datos generales</a>
                  </li>
                  {{!--
                    //Comentado para ocultar puntaje

                  <li>
                      <a class="page-scroll" href="#calidad">Calidad</a>
                  </li>
                  --}}
                  <li>
                      <a class="page-scroll" href="#planeacion">Planeación</a>
                  </li>
                  <li>
                      <a class="page-scroll" href="#licitacion">Convocatoria</a>
                  </li>
                  <li>
                      <a class="page-scroll" href="#adjudicaciones">Adjudicaciones</a>
                  </li>
                  <li>
                      <a class="page-scroll" href="#contratos">Contratos</a>
                  </li>
                  <li>
                      <a class="page-scroll" href="#implementacion">Implementación</a>
                  </li>
                  <div class="mt-2">
                    <a href="{{result.references}}" class="button-link">VER EN COMPRANET</a>
                  </div>
              </ul>
          </div>
      </div>


      <div class="col-m-8 col-8">
        <hr>
      </div>

      {{!--
        //Comentado para ocultar puntaje

      <div class="col-m-8 col-8 {{#if flags }}hidden{{else}}{{#if isPublic }}{{else}}hidden{{/if}}{{/if}}">
