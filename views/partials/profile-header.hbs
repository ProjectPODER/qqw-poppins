<section class="profile-header">
  <div class="wrap bg-white" id="wrap">
    <div class="description-data-title">
      <div class="classification-country">
        {{{ get_classification_icon (or result.classification (or result.type "record")) result.subclassification result.govLevel}}}
        {{!-- Type --}}
        <a href="{{url view="buscador" collection=type tipo-entidad=result.classification subtipo-entidad=result.subclassification}}">
            {{ get_classification_name type result.classification result.subclassification result.govLevel }}
        </a>
      {{!-- Area --}}
        {{#each result.area }}
          {{#if id }}
            {{#ifCond @key "==" 0}}en{{else}},{{/ifCond}}

            {{#ifCond this.classification "==" "country" }}
              <span class="flag-icon flag-icon-{{id}}"></span>
            {{/ifCond}}
            <a href="{{url view="regiones/:id" _id=id}}">
              {{translate_area name lang}}
            </a>
          {{/if}}
        {{/each}}
      </div>
      <h3 title="{{ result.name}}" style="word-break: break-word;">
        <span class="classification-icon" style="display: none;">{{{ get_classification_icon (or result.classification (or result.type "record")) result.subclassification
        result.govLevel}}}</span>
        {{#if result.area }}
        {{#each result.area }}
          {{#if id }}
            <span class="flag-icon-title flag-icon flag-icon-{{id}}" style="display: none;"></span>
          {{/if}}
        {{/each}}
        {{/if}}
        {{ or result.name (or result.contracts.title result.tender.title) }}
      </h3>
    </div>
    <div class="right-box ml-auto filters">
      <ul class="nav nav-pills d-flex flex-nowrap">
        {{>utilities api_url=(or result.api_url api_url) share_url=share_url share_url_encoded=share_url_encoded api_summary_url=result.api_summary_url }}
      </ul>
    </div>
    <nav id="right-menu">
      <ul class="right-menu-profile nav d-flex flex-nowrap">
        {{!-- Co-Branding --}}
        {{#each (cobranding result.source "data_name")}}
          <li>
            <span class="anchor cobranding-logo" title="{{ description }}">
              <img src="{{image}}" alt="{{data_name }}">
            </span>
          </li>
        {{/each}}