{{!-- j (get_concat_item result type "-top5") --}}
<table class="ui table unstackable table-responsive" id="table-{{type}}-top5">
  <thead>
    <tr>
      <th>Posici贸n</th>
      <th>Instituci贸n</th>
      <th >Transparencia</th>
      <th >Temporalidad</th>
      <th >Competitividad</th>
      <th >Trazabilidad</th>
      <th >Confiabilidad</th>
      <th >Monto total</th>
      <th>Puntaje</th>
    </tr>
  </thead>
  <tbody>
    {{#each (toloco_get_concat_item result type "-top5") }}
    <tr>
      <td class="td-number-right">{{#if (get_ranking flags.[0].ranking ../type)}}{{get_ranking flags.[0].ranking ../type}}{{else}}{{math @index "+" 1 }}{{/if}}</td>
      <td> <a href="https://www.quienesquien.wiki/instituciones-publicas/{{id}}" target="_blank">{{name}}</a></td>
      <td class="td-number">{{format_score flags.0.contract_categories.trans}}</td>
      <td class="td-number">{{format_score flags.0.contract_categories.temp}}</td>
      <td class="td-number">{{format_score flags.0.category_score.comp}}</td>
      <td class="td-number">{{format_score flags.0.category_score.traz}}</td>
      <td class="td-number">{{format_score flags.0.node_rules.conf}}</td>
      <td class="td-number-right">{{format_amount contract_amount.buyer}}</td>
      <td class="td-number">{{format_score flags.0.total_score}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
{{#if (toloco_get_concat_item result type "-bottom5") }}

